{
  "name": "vite-sandbox",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon --watch src/server --ext js,mjs,cjs,json,ts,tsx --exec npm run start:server",
    "start:server": "ts-node --project=tsconfig.server.json src/server/index.ts",
    "build": "run-s build:client build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "esbuild src/server/index.ts --bundle --platform=node --external:vite --tsconfig=tsconfig.server.json --outdir=dist",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "npm run build && node dist/index.js"
  },
  "packageManager": "pnpm@9.1.4",
  "dependencies": {
    "@types/express": "4.17.21",
    "@types/node": "20.12.7",
    "@types/react": "18.2.66",
    "@types/react-dom": "18.2.22",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.2.0",
    "@vitejs/plugin-react": "4.2.1",
    "body-parser": "1.20.2",
    "esbuild": "0.21.4",
    "eslint": "8.57.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.6",
    "express": "4.19.2",
    "node-fetch": "3.3.2",
    "nodemon": "3.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sirv": "2.0.4",
    "socket.io": "4.7.5",
    "sqlite": "5.1.1",
    "sqlite3": "5.1.7",
    "ts-node": "10.9.2",
    "typescript": "5.2.2",
    "vite": "5.2.0"
  }
}
